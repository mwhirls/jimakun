{
    "manifest_version": 3,
    "name": "Jimakun",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "js": [
                "content.bundle.js"
            ],
            "matches": [
                "https://www.google.com/*"
            ]
        }
    ]
}